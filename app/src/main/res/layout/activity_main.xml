<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/activity_main_rl_content"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".domains.main.MainActivity"
        android:background="@android:color/white">

    <ImageView
            android:id="@+id/activity_main_iv_menu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="19.2dp"
            android:paddingRight="19.2dp"
            android:paddingTop="8.8dp"
            android:paddingBottom="8.8dp"
            android:src="@drawable/ic_black_menu"/>

    <TextView
            android:id="@+id/activity_main_health_worker_tv_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="MAIDI"
            android:textSize="30dp"
            android:layout_marginTop="58.9dp"
            android:textColor="@color/dark_blue"
            android:layout_centerHorizontal="true"
            fontPath="fonts/NunitoSans-Bold.ttf"
    />

    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="29dp"
            android:layout_marginRight="29dp"
            android:layout_marginTop="40dp"
            android:orientation="vertical"
            android:layout_below="@+id/activity_main_health_worker_tv_title"
    >

        <LinearLayout android:layout_width="match_parent" android:layout_height="126.7dp">

            <FrameLayout
                    android:id="@+id/activity_main_health_worker_fl_child_registration"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.5"
                    android:background="@drawable/button_gray_gray_stroke_selector"
                    android:padding="12.5dp"
            >
                <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="@string/child_registration"
                        android:textSize="15.4dp"
                        android:textColor="@color/dark_blue"
                        android:gravity="center"
                        fontPath="fonts/NunitoSans-Bold.ttf"/>
            </FrameLayout>
            
            <View android:layout_width="14.4dp" android:layout_height="match_parent"/>

            <FrameLayout
                    android:id="@+id/activity_main_health_worker_fl_immunization_card"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.5"
                    android:background="@drawable/button_gray_gray_stroke_selector"
                    android:padding="12.5dp"
            >
                <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="@string/immunization_card"
                        android:textSize="15.4dp"
                        android:textColor="@color/dark_blue"
                        android:gravity="center"
                        fontPath="fonts/NunitoSans-Bold.ttf"/>
            </FrameLayout>


        </LinearLayout>

        <View android:layout_width="match_parent" android:layout_height="14.4dp"/>

        <LinearLayout android:layout_width="match_parent" android:layout_height="126.7dp">

            <FrameLayout
                    android:id="@+id/activity_main_health_worker_fl_monthly_workplan"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.5"
                    android:background="@drawable/button_gray_gray_stroke_selector"
                    android:padding="12.5dp"
            >
                <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="@string/monthly_workplan"
                        android:textSize="15.4dp"
                        android:textColor="@color/dark_blue"
                        android:gravity="center"
                        fontPath="fonts/NunitoSans-Bold.ttf"/>
            </FrameLayout>

            <View android:layout_width="14.4dp" android:layout_height="match_parent"/>

            <FrameLayout
                    android:id="@+id/activity_main_health_worker_fl_survey"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.5"
                    android:background="@drawable/button_gray_gray_stroke_selector"
                    android:padding="12.5dp"
            >
                <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="@string/survey"
                        android:textSize="15.4dp"
                        android:textColor="@color/dark_blue"
                        android:gravity="center"
                        fontPath="fonts/NunitoSans-Bold.ttf"/>
            </FrameLayout>


        </LinearLayout>

        <View android:layout_width="match_parent" android:layout_height="14.4dp"/>

        <LinearLayout android:layout_width="match_parent" android:layout_height="126.7dp">

            <FrameLayout
                    android:id="@+id/activity_main_health_worker_fl_aefi"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.5"
                    android:background="@drawable/button_gray_gray_stroke_selector"
                    android:padding="12.5dp"
            >
                <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="@string/aefi"
                        android:textSize="15.4dp"
                        android:textColor="@color/dark_blue"
                        android:gravity="center"
                        fontPath="fonts/NunitoSans-Bold.ttf"/>
            </FrameLayout>

            <View android:layout_width="14.4dp" android:layout_height="match_parent"/>

            <FrameLayout
                    android:id="@+id/activity_main_health_worker_fl_iec_bcc"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.5"
                    android:background="@drawable/button_gray_gray_stroke_selector"
                    android:padding="12.5dp"
            >
                <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="@string/iec_bcc"
                        android:textSize="15.4dp"
                        android:textColor="@color/dark_blue"
                        android:gravity="center"
                        fontPath="fonts/NunitoSans-Bold.ttf"/>
            </FrameLayout>


        </LinearLayout>

    </LinearLayout>

</RelativeLayout>